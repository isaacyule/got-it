<h1>Profile page</h1>
<h2> <%= current_user.email %> </h2>
<p> <%= link_to "Edit", edit_user_path %> </p>

<h4>My Listed Products</h4>
<div class="container">
  <div class="row">
    <% (Product.where(user_id: 15)).each do |product|%>
    <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
      <%= product.name %>
    </div>
    <% end %>
  </div>
</div>
